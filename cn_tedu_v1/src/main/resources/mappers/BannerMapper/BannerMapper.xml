<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tedu.cn_tedu_v1.index.zhangxin.dao.persist.mapper.BannerMapper">
    <select id="select"
            resultType="cn.tedu.cn_tedu_v1.index.zhangxin.pojo.vo.BannerVO">
        SELECT
               id, img_url imgUrl,title
        FROM
             zhipian_shop.zhipian_banner
        ORDER BY
                 sort
    </select>

</mapper>










