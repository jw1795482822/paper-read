<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tedu.cn_tedu_v1.ArticleDetailsPage.persist.mapper.BookOrdersMapper">

    <select id="SelectBytRadeStatus" resultType="java.lang.Integer">
        SELECT
            trade_status tradeStatus
        FROM zhipian_orders
        WHERE
        user_id =#{userId} AND book_ID = #{bookID}
    </select>



</mapper>










